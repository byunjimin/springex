<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="cafe.jjdev.web.service.FileMapper">
  <insert id="insertFile" parameterType="cafe.jjdev.web.service.FileVo">
        INSERT INTO file(
            fileTitle
            , filePath) 
        VALUES(
            #{fileTitle}
            , #{filePath}
        )
    </insert>
</mapper>